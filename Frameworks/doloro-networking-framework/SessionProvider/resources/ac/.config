<?xml version="1.0"?>
<Config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <UsersStorageDirectory>resorces\users\</UsersStorageDirectory>
  <LoginMinSize>5</LoginMinSize>
  <LoginMaxSize>16</LoginMaxSize>
  <UserNameRegexPattern>^([A-Z][a-z]+)+([-' ][A-Z][a-z]+)?$</UserNameRegexPattern>
  <UserDefaultRights>
    <string>rank=1</string>
    <string>base</string>
    <string>commenting</string>
    <string>publishing</string>
    <string>personManaging</string>
    <string>passwordManaging</string>
  </UserDefaultRights>
  <PasswordSaltFileName>.salt</PasswordSaltFileName>
  <PasswordSaltSize>32</PasswordSaltSize>
  <PasswordMinAllowedLength>8</PasswordMinAllowedLength>
  <PasswordMaxAllowedLength>32</PasswordMaxAllowedLength>
  <PasswordRequireSpecialSymbol>false</PasswordRequireSpecialSymbol>
  <PasswordRequireUpperSymbol>true</PasswordRequireUpperSymbol>
  <PasswordRequireDigitSymbol>true</PasswordRequireDigitSymbol>
  <TokenValidTimeMinutes>1440</TokenValidTimeMinutes>
  <QUERY_UserBan_RIGHTS>
    <string>banhammer</string>
    <string>&gt;rank=2</string>
  </QUERY_UserBan_RIGHTS>
  <QUERY_UserNewPassword_RIGHTS>
    <string>passwordManaging</string>
  </QUERY_UserNewPassword_RIGHTS>
  <QUERY_UserPasswordModeration_RIGHTS>
    <string>&gt;rank=2</string>
  </QUERY_UserPasswordModeration_RIGHTS>
  <QUERY_SetTokenRights_RIGHTS>
    <string>&gt;rank=5</string>
  </QUERY_SetTokenRights_RIGHTS>
</Config>